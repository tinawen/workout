<h1>Listing log_entries</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Date</th>
    <th>Exercise</th>
    <th>Duration</th>
    <th>Comment</th>
    <th>Points</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @log_entries.each do |log_entry| %>
  <tr>
    <td><%= log_entry.name %></td>
    <td><%= log_entry.date %></td>
    <td><%= log_entry.exercise %></td>
    <td><%= log_entry.duration %></td>
    <td><%= log_entry.comment %></td>
    <td><%= log_entry.points %></td>
    <td><%= link_to 'Show', log_entry %></td>
    <td><%= link_to 'Edit', edit_log_entry_path(log_entry) %></td>
    <td><%= link_to 'Destroy', log_entry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Log entry', new_log_entry_path %>
